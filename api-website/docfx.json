{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**/*.csproj"],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "test/**",
            "examples/**",
            "src/**/*Test*.csproj"
          ],
          "src": "../"
        }
      ],
      "filter": "docfx-filter-config.yml",
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": ["*.md", "toc.yml"]
      }
    ],
    "resource": [
      {
        "files": ["assets/**", "_redirects", "serve.json"]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "GraphZen API",
      "_appFooter": "<span>Copyright Â© 2017-2019 GraphZen LLC</span>",
      "_enableSearch": false,
      "_disableAffix": true,
      "_disableContribution": true
    },
    "markdownEngineName": "markdig",
    "dest": "../api-website-artifacts/dist",
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"],
    "template": ["default", "custom-template"]
  }
}
