<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <Description>Object model for the GraphQL type system for creating, inspecting and manipulating GraphQL schemas.</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Superpower" Version="2.2.0" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard2.0' ">
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\GraphZen.LanguageModel\GraphZen.LanguageModel.csproj" />
  </ItemGroup>
</Project>
